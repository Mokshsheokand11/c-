  cout << a << endl;
  char b = 'm';
  cout << b << endl;
  bool bl = true;
  cout << bl << endl;
  float f = 1.23;
  cout << f << endl;
  int size = sizeof(a);
  cout << "size of a is : " << size << endl;
cout<< "myfile" << endl;
 }
